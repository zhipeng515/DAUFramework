{
    
    "layoutInfo":
    {
        "rootView":
        {
            "creator":"createView",
            "property":
            {
                "frame":"{0,0,320,568}",
                "bgcolor":"{255,255,255,255}"
            }
        },
        "debugInfo":
        {
            "creator":"createTextView",
            "property":
            {
                "frame":"{0,0,320,568}",
                "bgcolor":"{255,255,255,255}"
            }
        },
        "logoView":
        {
            "creator":"createImageView",
            "property":
            {
                "frame":"{0,0,200,200}",
                "bgcolor":"{255,255,255,255}"
            }
        },
        "userNameText":
        {
            "creator":"createTextField",
            "property":
            {
                "frame":"{10,230,100,30}",
                "bgcolor":"{255,255,255,255}"
            }
        },
        "userPasswordText":
        {
            "creator":"createTextField",
            "property":
            {
                "frame":"{10,270,100,30}",
                "bgcolor":"{255,255,255,255}"
            }
        },
        "registerButton":
        {
            "creator":"createButton",
            "property":
            {
                
            }
        }
    },
    "dataInfo":
    {
        "userInfo":
        {
            "creator":"createData",
            "model":"userModel",
            "property":
            {
                "userId":"",
                "userName":"",
                "userPassword":"",
                "mobilePhone":""
            }
        }
    },
    "actionInfo":
    {
        "requestVerifyCode":
        {
            "creator":"createHttpAction",
            "property":
            {
                "url":"/userSms/sendByRegister",
                "param":
                {
                    "mobilePhone":"$userInfo.userName",
                    "api_key":"verify"
                }
            },
            "complete":"",
            "failed":""
        },
        "sendRegister":
        {
            "creator":"createHttpAction",
            "property":
            {
                "url":"/user/register",
                "param":
                {
                    "loginUserId":"$userInfo.userId",
                    "mobilePhone":"$userInfo.mobilePhone",
                    "password":"$user.userPassword",
                    "idenCode":"$globalData.verifyCode",
                    "deviceType":"$globalData.deviceType",
                    "deviceId":"globalData.deviceId",
                    "api_key":"verify"
                }
            },
            "complete":"registerComplete",
            "failed":"registerFailed"
        },
        "registerComplete":
        {
            "creator":"createCustomAction",
            "property":
            {
            },
            "complete":"",
            "failed":""
        },
        "registerFailed":
        {
            "creator":"createCustomAction",
            "property":
            {
            },
            "complete":"",
            "failed":""
        }
    },
    "binderInfo":
    {
        "$registerButton":"$sendRegister",
        "$userNameText":"$userInfo.mobilePhone",
        "$userPasswordText":"$userInfo.userPassword"
    }
}
